<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>numbworks</Authors>
    <Company>numbworks</Company>
    <Product>unifor</Product>
    <Copyright>numbworks 2023</Copyright>
    <Description>unifor is a command-line application to perform univariate forecasting tasks based on the NW.UnivariateForecasting library.</Description>
    <PackageProjectUrl>https://github.com/numbworks/NW.UnivariateForecasting</PackageProjectUrl>
    <RepositoryUrl>https://github.com/numbworks/NW.UnivariateForecasting</RepositoryUrl>
    <PackageTags>machinelearning csharp library univariate timeseriesanalysis netstandard forecasting</PackageTags>
    <PackageIconUrl></PackageIconUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Version>2.5.0</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<AssemblyVersion>2.5.0.0</AssemblyVersion>
	<FileVersion>2.5.0.0</FileVersion>
	<PackageIcon>numbworks_logo_128x128px.png</PackageIcon>
	<ApplicationIcon>numbworks_logo_128x128px.ico</ApplicationIcon>
	<AssemblyName>unifor</AssemblyName>		
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\NW.UnivariateForecasting\NW.UnivariateForecasting.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\numbworks_logo_128x128px.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="McMaster.Extensions.CommandLineUtils" Version="4.0.1" />
  </ItemGroup>

</Project>
